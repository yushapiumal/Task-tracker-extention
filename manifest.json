{
  "manifest_version": 3,
  "name": "Task Tracker Extension",
  "version": "1.1",
  "description": "A Chrome extension for managing tasks",
 "permissions": [
  "scripting",
  "storage",
  "notifications",
  "activeTab"
],
 "host_permissions": [
  "https://dtbl.go.digitable.io/*"
],
"background": {
    "service_worker": "background.js"
  },
"action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon/icon16.png",
      "48": "icon/icon48.png",
      "128": "icon/icon128.png"
    }
  },
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  }
}


